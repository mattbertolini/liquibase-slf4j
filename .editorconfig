# EditorConfig file for liquibase-slf4j

root = true

[*]
end_of_line = lf
insert_final_newline = true

[*.{java,xml}]
charset = utf-8
indent_style = space
indent_size = 4

[*.java]
ij_java_class_count_to_use_import_on_demand = 999
ij_java_names_count_to_use_import_on_demand = 999

[*.{yml,yaml}]
charset = utf-8
indent_style = space
indent_size = 2
